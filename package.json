{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "A platform to share exciting recipe ideas",
  "main": "index.js",
  "scripts": {
    "test": "npm run seed:db && nyc --reporter=html --reporter=text mocha ./server/tests/api/user --compilers js:babel-register --timeout 1500ms",
    "start": "nodemon ./server/app.js --exec babel-node",
    "setup:seed":"npm run migrations && sequelize db:seed:all",
    "seed:db": "npm run migrations && set NODE_ENV=test sequelize db:seed:all",
    "migrations": "set NODE_ENV=test sequelize db:migrate:undo:all && set NODE_ENV=test sequelize db:migrate",
    "build": "babel ./server/app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "recipe"
  ],
  "author": "Valentine Ezeh",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "Faker": "^0.7.2",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.2",
    "body-parser": "~1.17.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "jsonwebtoken": "^7.4.2",
    "mocha": "^3.5.0",
    "morgan": "~1.8.1",
    "nodemon": "^1.11.0",
    "pg": "^7.2.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.8.0",
    "supertest": "^3.0.0",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.19.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "mocha-istanbul": "^0.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.1.0"
  }
}
