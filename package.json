{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "A platform to share exciting recipe ideas",
  "main": "index.js",
  "scripts": {
    "test": "npm run seed:db && nyc --reporter=html --reporter=text mocha ./server/test/usersTest --compilers js:babel-register --timeout 1500ms",
    "start": "nodemon ./server/app.js --exec babel-node",
    "setup:seed": "npm run migrations && sequelize db:seed:all",
    "seed:db": "npm run migrations && set NODE_ENV=test sequelize db:seed:all",
    "migrations": "set NODE_ENV=test sequelize db:migrate:undo:all && set NODE_ENV=test sequelize db:migrate",
    "build": "babel ./client/client.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "postinstall": "npm run build",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "recipe"
  ],
  "author": "Valentine Ezeh",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "Faker": "^0.7.2",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.2",
    "body-parser": "~1.17.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^7.4.2",
    "loadash": "^1.0.0",
    "lodash.isempty": "^4.4.0",
    "mocha": "^3.5.0",
    "morgan": "~1.8.1",
    "nodemon": "^1.11.0",
    "pg": "^7.2.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.0-rc.3",
    "react-router-dom": "^4.3.0-rc.3",
